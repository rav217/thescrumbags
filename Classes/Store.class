//Store class:

public class Store{
	
	private ProductCatalog catalog;
	private Register register;
	
	//constructor
	public Store(){
		this.catalog = new ProductCatalog();
		this.register = new Register(catalog);
	}
	
	//get catalog
	public getCatalog(){
		return this.catalog;
	}
	
	//get register
	public getRegister(){
		return this.register;
	}
}